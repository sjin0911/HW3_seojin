    "def factorial(x):\n",
    "    if x>=1:\n",
    "        return x*factorial(x-1);\n",
    "    else: \n",
    "        return 1;\n",
    "    \n",
    "def main():\n",
    "    for i in range(0,15,2):\n",
    "        print(factorial(i))\n",
    "if __name__ == '__main__':\n",
    "    \n",
    "    main()"
   ]
